<div class="container mt-5">

  <h1>New Restaurants</h1>

  <%= simple_form_for @restaurant do |f| %>
    <%= f.input :name%>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category %>
    <%= f.button :submit %>
  <% end %>


    <br>
    <%= link_to restaurants_path do %>
      <i class="fa-solid fa-arrow-rotate-left"></i> Back to list
    <% end %>
</div>
